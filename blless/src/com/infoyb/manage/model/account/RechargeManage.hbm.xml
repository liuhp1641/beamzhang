<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cm.manage.model.account.RechargeManage" table="ACCOUNT_RECHARGE_MANAGE">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">SEQ_ACCOUNT_RECHARGE_MANAGE</param>
			</generator>
		</id>
		<property name="channelCode" column="CHANNEL_CODE" type="java.lang.String" length="50" />
		<property name="channelName" column="CHANNEL_NAME" type="java.lang.String" length="50" />
		<property name="label" column="LABEL" type="java.lang.String" length="50"/>
		<property name="adNote" column="AD_NOTE" type="java.lang.String" length="40" />
		<property name="remark" column="REMARK" type="java.lang.String" length="50" />
		<property name="status" column="STATUS" type="java.lang.Integer" />
		<property name="createTime" column="CREATE_TIME" type="java.util.Date" />
		<property name="updateTime" column="UPDATE_TIME" type="java.util.Date" />
	</class>
</hibernate-mapping>