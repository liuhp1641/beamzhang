<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cm.manage.model.order.ProgramsReCode" table="TMS_PROGRAMS_RE_CODE">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence">SEQ_TMS_PROGRAMS_RE_CODE</param>
            </generator>
        </id>
        <property name="programsOrderId" column="PROGRAMS_ORDER_ID" type="java.lang.String" length="20"/>
        <property name="sid" column="SID" type="java.lang.String" length="20"/>
        <property name="userCode" column="USER_CODE" type="java.lang.String" length="20"/>
        <property name="lotteryCode" column="LOTTERY_CODE" type="java.lang.String" length="3"/>
        <property name="issue" column="ISSUE" type="java.lang.String" length="10"/>
        <property name="matchId" column="MATCH_ID" type="java.lang.String" length="20"/>
        <property name="createTime" column="CREATE_TIME" type="java.util.Date"/>
        <property name="endTime" column="END_TIME" type="java.util.Date"/>
    </class>
</hibernate-mapping>