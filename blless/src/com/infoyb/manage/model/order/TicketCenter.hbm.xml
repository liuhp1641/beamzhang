<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cm.manage.model.order.TicketCenter" table="TMS_TICKET_CENTER">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence">SEQ_TMS_TICKET_CENTER</param>
            </generator>
        </id>

        <property name="postCode" column="POST_CODE" type="java.lang.String" length="2"/>
        <property name="postName" column="POST_NAME" type="java.lang.String" length="50"/>
        <property name="balance" column="BALANCE" type="java.lang.Double" length="15" scale="2"/>

    </class>
</hibernate-mapping>