<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cm.manage.model.rebate.TaskRebateUser" table="TASK_REBATE_USER">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">SEQ_TASK_REBATE_USER</param>
			</generator>
		</id>
		<property name="rebateUserId" column="REBATE_USER_ID" type="java.lang.String" length="30" />
		<property name="rebateId" column="REBATE_ID" type="java.lang.String" length="30" />
		<property name="rebateName" column="REBATE_NAME" type="java.lang.String" length="100" />
		<property name="userCode" column="USER_CODE" type="java.lang.String" length="30" />
		<property name="outUserCode" column="OUT_USER_CODE" type="java.lang.String" length="30" />
		<property name="lotteryCode" column="LOTTERY_CODE" type="java.lang.String" length="10" />
		<property name="orderId" column="ORDER_ID" type="java.lang.String" length="30" />
		<property name="type" column="TYPE" type="java.lang.Integer" />
		<property name="vip" column="VIP" type="java.lang.Integer" />
		<property name="amount" column="AMOUNT" type="java.lang.Double" length="15" scale="2" />
		<property name="scoreRate" column="SCORE_RATE" type="java.lang.Double" length="15" scale="2" />
		<property name="returnScore" column="RETURN_SCORE" type="java.lang.Double" length="15" scale="2" />
		<property name="preRate" column="PRE_RATE" type="java.lang.Double" length="15" scale="2" />
		<property name="returnPre" column="RETURN_PRE" type="java.lang.Double" length="15" scale="2" />
		<property name="status" column="STATUS" type="java.lang.Integer" />
		<property name="createTime" column="CREATE_TIME" type="java.util.Date" />
		<property name="updateTime" column="UPDATE_TIME" type="java.util.Date" />
	</class>
</hibernate-mapping>