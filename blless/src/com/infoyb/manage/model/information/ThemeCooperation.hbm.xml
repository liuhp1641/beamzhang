<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cm.manage.model.information.ThemeCooperation" table="INFO_THEME_COOPERATION">
        <id name="id" column="ID"  unsaved-value="null">
            <generator class="sequence">
                <param name="sequence">info_theme_COOPERATION_seq</param>
            </generator>
        </id>
        <property name="themeCode" column="THEME_CODE" type="java.lang.String"/>
        <property name="sid" column="COOPERATION_SID" type="java.lang.String"/>
        <property name="createDate" column="CREATEDATE" type="java.util.Date"/>
    </class>
</hibernate-mapping>