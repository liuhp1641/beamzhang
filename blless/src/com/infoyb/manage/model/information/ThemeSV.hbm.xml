<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cm.manage.model.information.ThemeSV" table="INFO_THEME_SV">
        <id name="id" column="ID"  unsaved-value="null">
            <generator class="sequence">
                <param name="sequence">info_theme_sv_Seq</param>
            </generator>
        </id>
        <property name="themeCode" column="THEME_CODE" type="java.lang.String"/>
        <property name="svCode" column="SV_CODE" type="java.lang.String"/>
       
    </class>
</hibernate-mapping>