<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cm.manage.model.score.ScoreQutzOption" table="SCORE_QUTZ_OPTION">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">SEQ_SCORE_QUTZ_OPTION</param>
			</generator>
		</id>
		<property name="qutzId" column="QUTZ_ID" type="java.lang.String" length="30" />
		<property name="optionId" column="OPTION_ID" type="java.lang.String" length="255" />
		<property name="optionNote" column="OPTION_NOTE" type="java.lang.String" length="255" />
		<property name="optionOrder" column="OPTION_ORDER" type="java.lang.Integer" />
		<property name="attendNumbers" column="ATTEND_NUMBERS" type="java.lang.Integer" />
		<property name="attendAmount" column="ATTEND_AMOUNT" type="java.lang.Double" length="15" scale="2" />
		<property name="createTime" column="CREATE_TIME" type="java.util.Date" />
		<property name="updateTime" column="UPDATE_TIME" type="java.util.Date" />
	</class>
</hibernate-mapping>
