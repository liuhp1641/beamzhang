<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cm.manage.model.score.ScoreQutzUserCount" table="SCORE_QUTZ_USER_COUNT">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">SEQ_SCORE_QUTZ_USER_COUNT</param>
			</generator>
		</id>
		<property name="userCode" column="USER_CODE" type="java.lang.String" length="30" />
		<property name="qutzId" column="QUTZ_ID" type="java.lang.String" length="30" />
		<property name="firstOptionNote" column="FIRST_OPTION_NOTE" type="java.lang.String" length="255" />
		<property name="firstOptionId" column="FIRST_OPTION_ID" type="java.lang.String" length="30" />
		<property name="attendOptions" column="ATTEND_OPTIONS" type="java.lang.Integer" />
		<property name="amount" column="AMOUNT" type="java.lang.Double" />
		<property name="bonusAmount" column="BONUS_AMOUNT" type="java.lang.Double" />
		<property name="createTime" column="CREATE_TIME" type="java.util.Date" />
		<property name="updateTime" column="UPDATE_TIME" type="java.util.Date" />
	</class>
</hibernate-mapping>