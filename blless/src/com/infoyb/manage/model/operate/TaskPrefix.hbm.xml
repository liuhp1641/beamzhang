<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cm.manage.model.operate.TaskPrefix" table="TASK_PREFIX_CONDITION">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence">SEQ_SYS_TASK_PREFIX</param>
            </generator>
        </id>
        <property name="taskId" column="TASK_ID" type="java.lang.String" length="30"/>
        <property name="type" column="TYPE" type="java.lang.Integer" length="1"/>
        <property name="times" column="times" type="java.lang.Integer" length="5"/>
        <property name="amount" column="AMOUNT" type="java.lang.Double" length="15" scale="2"/>
        <property name="totalAmount" column="TOTAL_AMOUNT" type="java.lang.Double" length="15" scale="2"/>
        <property name="firstAmount" column="FIRST_AMOUNT" type="java.lang.Double" length="15" scale="2"/>       
        <property name="prefixTaskId" column="prefix_task_id" type="java.lang.String" length="30"/>       
        <property name="backup1" column="BACKUP1" type="java.lang.String" length="50"/>
        <property name="backup2" column="BACKUP2" type="java.lang.String" length="100"/>
        <property name="backup3" column="BACKUP3" type="java.lang.String" length="150"/>
        <property name="lotteryType" column="LOTTERY_TYPE" type="java.lang.String" length="20"/>
        <property name="lotteryTypeName" column="LOTTERY_TYPE_NAME" type="java.lang.String" length="50"/>
        <property name="vipRank" column="VIP_RANK" type="java.lang.Integer" length="1"/>
        <property name="listType" column="LIST_TYPE" type="java.lang.Integer" length="1"/>
        <property name="completeFlag" column="COMPLETE_FLAG" type="java.lang.Integer" length="1"/>
        <property name="sid" column="sid" type="java.lang.String" length="30"/>
        <property name="sidName" column="sid_name" type="java.lang.String" length="50"/>
        <property name="prefixTaskName" column="PREFIX_TASK_NAME" type="java.lang.String" length="30"/>
        <property name="createTime" column="CREATE_TIME" type="java.util.Date"/>
    </class>
</hibernate-mapping>