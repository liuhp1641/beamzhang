<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cm.manage.model.operate.SoftwareVersion" table="SYS_SOFTWARE_VERSION">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence">sys_software_version_Seq</param>
            </generator>
        </id>
        <property name="svCode" column="SV_CODE" type="java.lang.String" />
        <property name="svType" column="SV_TYPE" type="java.lang.String" />
        <property name="svName" column="SV_NAME" type="java.lang.String" />
        
    </class>
</hibernate-mapping>