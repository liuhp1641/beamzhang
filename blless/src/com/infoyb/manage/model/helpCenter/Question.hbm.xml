<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cm.manage.model.helpCenter.Question" table="HC_QUESTION">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence">hc_Question_id</param>
            </generator>
        </id>
        <property name="userCode" column="USER_CODE" type="java.lang.String"/>
        
        <property name="createDate" column="CreateDate" type="java.util.Date"/>
        <property name="content" column="Content" type="java.lang.String"/>
        <property name="state" column="State" type="java.lang.Integer" />
        <property name="remark" column="remark" type="java.lang.String" />
        <property name="sourceType" column="sourceType" type="java.lang.Integer" />
        <property name="equipmentType" column="Equipment_Type" type="java.lang.String" />
        <property name="equipmentSystem" column="EQUIPMENT_System" type="java.lang.String" />
        <property name="answerNum" column="Answer_Num" type="java.lang.Integer" />
        <property name="questionTypeId" column="question_Type_Id" type="java.lang.Long" />
        <property name="lastDate" column="LastDate" type="java.util.Date" />
        <property name="updateDate" column="updateDate" type="java.util.Date" />
        <property name="weight" column="weight" type="java.lang.Integer"/>
        <property name="pageView" column="Page_view" type="java.lang.Integer"/>
        <property name="hiddenFlag" column="Hidden_flag" type="java.lang.Integer"/>
        <property name="examineNum" column="EXAMINENUM" type="java.lang.Integer"/>
        <property name="unExamineNum" column="UNEXAMINENUM" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>