<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cm.manage.model.helpCenter.Picture" table="HC_PICTURE">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence">hc_Picture_id</param>
            </generator>
        </id>
        <property name="questionId" column="QUESTION_ID" type="java.lang.Long" />
        <property name="picturePath" column="PICTURE_PATH" type="java.lang.String"/>
    </class>
</hibernate-mapping>